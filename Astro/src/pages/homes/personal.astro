        { text: t("personal.header.home"), href: '#' },
        { text: t("personal.header.about"), href: '#about' },
        { text: t("personal.header.resume"), href: '#resume' },
        { text: t("personal.header.portfolio"), href: '#portfolio' },
        { text: t("personal.header.blog"), href: '#blog' },
        { text: t("personal.header.github"), href: 'https://github.com/ZenDeiAn' },
